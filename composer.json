{
    "name": "nathandunn/chargebee-laravel-sdk",
    "description": "Laravel Client for using the Chargebee API.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Nathan Dunn",
            "email": "github@nathandunn.uk",
            "role": "Developer",
            "homepage": "https://nathandunn.uk"
        }
    ],
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "5.6.*",
        "illuminate/support": "5.6.*",
        "graham-campbell/manager": "^3.0",
        "nathandunn/chargebee-php-sdk": "^0.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "graham-campbell/analyzer": "^2.0",
        "graham-campbell/testbench": "^5.0",
        "php-http/mock-client": "^1.1",
        "php-http/guzzle6-adapter": "^1.1",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "NathanDunn\\ChargebeeLaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "NathanDunn\\ChargebeeLaravel\\ServiceProvider"
            ],
            "aliases": {
                "Chargebee": "NathanDunn\\ChargebeeLaravel\\Facades"
            }
        }
    }
}
